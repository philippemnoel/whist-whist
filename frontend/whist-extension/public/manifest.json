{
  "name": "Whist Tab",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Whist extension to create cloud tabs",
  "icons": {},
  "background": {
    "service_worker": "src/worker.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["history", "identity", "scripting", "storage", "tabs"],
  "host_permissions": ["*://*/*"]
}
