{
  "name": "Cloud Tab",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Whist extension to create cloud tabs",
  "icons": {},
  "background": {
    "service_worker": "src/worker.js"
  },
  "chrome_url_overrides": {
    "login": "src/newtab.html"
  },
  "permissions": ["tabs"],
  "host_permissions": ["*://*/*"]
}
