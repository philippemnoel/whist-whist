        "directories": {
            "buildResources": "resources",
            "output": "release"
        },
        "dmg": {
            "contents": [
                {
                    "x": 130,
                    "y": 220
                },
                {
                    "path": "/Applications",
                    "type": "link",
                    "x": 410,
                    "y": 220
                }
