{
    "problemMatcher": [
        {
            "owner": "clang",
            "pattern": [
                {
                    "regexp": "^((?:.(?!include|lib|docs|sentry-native|share|nvidia-linux|fractalgetopt\\.[ch]))*):(\\d+):(\\d+):\\s+(warning|error):\\s+((?:.(?!clang-diagnostic-error)))*$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            ]
        }
    ]
}
