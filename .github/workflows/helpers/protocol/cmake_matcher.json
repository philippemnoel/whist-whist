{
  "problemMatcher": [
    {
      "owner": "cmake",
      "pattern": [
        {
          "regexp": "^\\s*CMake (Error|Warning) at (.+):(\\d+)\\s*(.*)?:",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}
