{
  "name": "fractal-auth0",
  "version": "1.0.0",
  "description": "Auth0 configuration and hooks",
  "repository": "https://github.com/fractal/fractal.git",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@esbuild-plugins/node-resolve": "^0.1.4",
    "@fractal/core-ts": "file:../../core-ts",
    "esbuild": "^0.11.19"
  },
  "scripts": {
    "build": "node ./scripts/build.js",
    "list-dependencies": "node ./scripts/list-dependencies.js",
    "update-tenant": "node ./scripts/update-tenant.js dev",
    "deploy:dev": "a0deploy import --input_file tenant.yaml --config_file ./config/dev.json",
    "deploy:staging": "a0deploy import --input_file tenant.yaml --config_file ./config/staging.json",
    "deploy:prod": "a0deploy import --input_file tenant.yaml --config_file ./config/prod.json",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "devDependencies": {
    "lodash": "^4.17.21",
    "yaml": "^1.10.2"
  }
}
